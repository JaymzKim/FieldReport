<?xml version="1.0" encoding="UTF-8"?>

<MOML version="1.1.5">
<UI>
    <UILAYOUT portrait="720,1280">
        <LIST layout="0,0,720,1280" align="linear:vertical" dataSource="http://www.ebunyang.co.kr/AjaxContent/?sy_ajax_content=SYFieldReportAPPList" dataList="/DATA/ITEMLIST/SYItem" divider="#00000000">
            <LISTLAYOUT align="relative">
                <WINDOW align="linear:vertical">
                    <IMAGE layout="0,0,720,auto" align="relative" defaultImg="{xpath.evaluate('./SYImage/text()')}"/>
                    <LABEL layout="25,parent.height-60,450,80" align="relative" text="{xpath.evaluate('./SYTitle/text()')}" fontSize="60" minFontSize="-1" textAlign="vCenter" fontStyle="bold" textColor="#ffffff" textShadow="#90000000"/>
                    <LABEL align="relative" text="{xpath.evaluate('./SYMemo/text()')}" fontSize="12" minFontSize="-1" fontStyle="bold" textColor="#ffffff" textShadow="#90000000"/>
                </WINDOW>
            </LISTLAYOUT>
        </LIST>

    </UILAYOUT>
</UI>
</MOML>



