<?xml version="1.0" encoding="UTF-8"?>

<MOML version="1.1.5">
<UI>
    <UILAYOUT portrait="720,1230" landscape="720,1230">
        <LIST layout="0,0,720,1230" align="linear:vertical" dataSource="http://www.ebunyang.co.kr/AjaxContent/?sy_ajax_content=SYFieldReportAPPList" dataList="/DATA/ITEMLIST/SYItem" divider="none">
            <LISTLAYOUT align="linear:vertical">
                <WINDOW defaultImg="#ffffff" align="flow:vertical">
                    <IMAGE layout="720,auto" defaultImg="{xpath.evaluate('./SYImage/text()')}"/>
                    <LABEL padding="25,-80,0,0" align="relative" text="{xpath.evaluate('./SYTitle/text()')}" fontSize="50" minFontSize="-1" fontStyle="bold" textColor="#ffffff" textShadow="#90000000"/>
					<WINDOW layout="720,auto">
					 <LABEL layout="720,auto" padding="20,40,20,40" textAlign="center|vCenter" text="{xpath.evaluate('./SYMemo/text()')}" fontSize="30" minFontSize="-1" multiLine="true" textColor="#585858" defaultImg="#ffffff"/>
					</WINDOW>
                </WINDOW>
            </LISTLAYOUT>
        </LIST>

    </UILAYOUT>
</UI>
</MOML>



