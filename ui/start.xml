<?xml version="1.0" encoding="UTF-8"?>

<MOML version="1.1.5">
<FUNCTION id="SYDetailPopup(sy_value)">
	<CMD cmd="userVariable.detailY = 10" />
	<CMD cmd="userVariable.sy_id = sy_value" />
	<CMD cmd="container.open('SYDetail.xml','new')" />
</FUNCTION>
<UI>
    <UILAYOUT portrait="720,1230" landscape="720,1230">
                <WINDOW defaultImg="#ffffff" align="flow:vertical">
					<WINDOW align="relative" onClick="function.SYDetailPopup('29')">
						<IMAGE layout="0,0,720,auto" align="linear:vertical" loadingEffect="fadeIn" loadingImg="#e5e5e5" defaultImg="#dddddd"/>
						<LABEL layout="25,prev.height-80,700,60" align="linear:vertical" text="www" fontSize="50" minFontSize="-1" fontStyle="bold" textColor="#ffffff" textShadow="#90000000"/>
					</WINDOW>
					<WINDOW align="flow:vertical|center">
						<LABEL layout="720,auto" padding="20,40,20,40" textAlign="vCenter|hCenter" text="sss" fontSize="30" minFontSize="-1" multiLine="true" textColor="#585858" defaultImg="#ffffff"/>
					</WINDOW>
                </WINDOW>
    </UILAYOUT>
</UI>
</MOML>



