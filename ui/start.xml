<?xml version="1.0" encoding="UTF-8"?>

<MOML version="1.1.5">
<UI>
    <UILAYOUT portrait="720,1230">
        <LIST layout="0,0,720,1230" align="linear:vertical" dataSource="http://www.ebunyang.co.kr/AjaxContent/?sy_ajax_content=SYFieldReportAPPList" dataList="/DATA/ITEMLIST/SYItem" divider="#00000000">
            <LISTLAYOUT align="relative">
                <WINDOW defaultImg="#ffffff">
                    <IMAGE layout="0,0,720,auto" align="linear:vertical" defaultImg="{xpath.evaluate('./SYImage/text()')}"/>
                    <LABEL layout="25,prev.height-80,700,60" align="linear:vertical" text="{xpath.evaluate('./SYTitle/text()')}" fontSize="50" minFontSize="-1" fontStyle="bold" textColor="#ffffff" textShadow="#90000000"/>
                    <LABEL layout="0,prev.prev.height+20,720,200" padding="20,20,20,20" align="linear:vertical" text="{xpath.evaluate('./SYMemo/text()')}" fontSize="34" minFontSize="-1" textColor="#585858" defaultImg="#ffffff"/>
                </WINDOW>
            </LISTLAYOUT>
        </LIST>

    </UILAYOUT>
</UI>
</MOML>



